.title{
  display: flex;
  padding: 50rpx 0 110rpx 150rpx;
  background-color: #c9ba83;
  color: #ffffff;
  align-items: center;
}

.title-left, .title-right{
  display: flex;
  flex-direction: column;
  width: 40%;
  text-align: center;
}

.nav{
  display: flex;
  margin: 0 30rpx ;
  padding: 40rpx 0 30rpx;
  margin-top: -80rpx;
  justify-content: space-around;
  background-color: #ffffff;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
}

.nav-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.nav-img{
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 10rpx;
}

.activity{
  margin: 20rpx;
}

.activity .van-cell{
  margin-bottom: 10rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
}





.font-40{
  font-size: 40rpx;
  font-weight: 550;
}