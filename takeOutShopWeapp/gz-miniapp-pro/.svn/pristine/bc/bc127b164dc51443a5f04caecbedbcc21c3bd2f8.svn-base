<view class="container">
  <view class="header">
    <view class="header-left">
      <text class="">今日预计收入(元)</text>
      <view class="header-left__num">300</view>
    </view>
    <view class="header-left">
      <text class="">今日预计支出</text>
      <view class="header-left__num">-3</view>
    </view>
  </view>
  <view class="middle van-hairline--bottom">
    <text>我的余额(元)</text>
    <view class="middle-content">
      <view class="middle-content__left">
        <text class="middle-left__num dis">438.22</text>
        <van-icon custom-class="value-icon" name="arrow" size="24rpx"></van-icon>
      </view>
      <van-button type="default">去提现</van-button>
    </view>
    <text class="">还有217.50即将到账(含今日的预支出)</text>
  </view>
  <view class="pledge">
    <van-cell title="保证金" is-link bindtap="onpledge"/>
  </view>
  <view class="footer">
    <van-cell title="今日收入明细" value="历史明细" is-link bindtap="billDetail" />
    <view class="footer-item van-hairline--top-bottom">
      <view class="item-top font-32-c">
        <view class="item-top-left van-ellipsis">平台服务费的力困筋乏了看家的说法卢克倒计时了看法就的</view>
        <view class="item-top-right">-3.00</view>
      </view>
      <view class="item-top font-24-9">
        <view class="item-bottom-left van-ellipsis">2018-10-28 06:54</view>
        <view class="item-bottom-right">预计在10-29号到账</view>
      </view>
    </view>
  </view>
</view>